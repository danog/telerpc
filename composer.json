        "amphp/php-cs-fixer-config": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "license": "AGPLV3",
    "authors": [
        {
            "name": "Daniil Gentili",
            "email": "daniil.gentili.dg@gmail.com"
        }
